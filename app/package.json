{
  "name": "app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon -r dotenv/config script/serve.js",
    "fixture": "node -r dotenv/config script/fixture.js",
    "test": "node -r dotenv/config script/test.js",
    "activate": "gcloud config configurations activate default",
    "image": "echo gcr.io/`gcloud config get-value project`/shelf-public:latest",
    "build": "rm -rf static && cp -r ../static/dist static && docker image build -t shelf-public:latest .",
    "tag": "docker image tag shelf-public:latest `npm run -s image`",
    "push": "docker image push `npm run -s image`",
    "deploy": "gcloud run deploy shelf-public --image `npm run -s image` --platform managed --region asia-northeast1",
    "staging": "npm run activate && npm run build && npm run tag && npm run push && npm run deploy",
    "dump": "mysqldump -u root shelf > dump.sql"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mysql-session": "^2.1.6",
    "express-session": "^1.17.1",
    "helmet": "^4.6.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "nocache": "^2.1.0",
    "proxy-middleware": "^0.15.0",
    "pug": "^3.0.2",
    "sequelize": "^6.6.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "node-fetch": "^2.6.1",
    "puppeteer": "^9.1.1"
  }
}
